<Window x:Class="Practicheskaia11.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practicheskaia11"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Результат"/>
            <ListBox x:Name="listBox" Height="200" Margin="5"/>
            <Button Click="btRegex" Content="Найти" HorizontalAlignment="Center" Margin="5"/>
            <Button Click="btFound" Content="Найти a[a-f]a" HorizontalAlignment="Center" Margin="5"/>
            <Button Click="btInfo" Content="Информация" Margin="5" HorizontalAlignment="Center"/>
            <Button Click="btExit" Content="Выход" Margin="5" HorizontalAlignment="Center"/>
        </StackPanel>
    </Grid>
</Window>
